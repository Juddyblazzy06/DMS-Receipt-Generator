{
  "name": "school-fee-receipt-generator",
  "version": "1.0.0",
  "description": "A MERN stack application for generating school fee receipts for Nigerian schools",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "receipt",
    "school",
    "fee",
    "generator",
    "nigeria",
    "mern",
    "react",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
} 